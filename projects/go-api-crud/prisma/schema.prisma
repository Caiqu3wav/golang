datasource db {
  // could be postgresql or mysql
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}
 
model Product {
  id          String   @default(cuid()) @id
  product_name String
  brand       String
  quantity    Int
  price       Float    
  desc String?
}
